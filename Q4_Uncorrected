#include<stdio.h>
int main(){
    struct c{
        int r[2];
        int i[2];
        int res[2];
    }c;
    printf("Enter the value for r1");
    scanf("%d",&c.r[0]);
    
    printf("Enter the value for r2");
    scanf("%d",&c.r[1]);
    
    printf("Enter the value for i1");
    scanf("%d",&c.i[0]);
    
    printf("Enter the value for i2");
    scanf("%d",&c.i[0]);
    
    res[0]=c.r[0]*c.r[1];
    res[1]=c.i[0]*c.i[1];
    
    printf("Result 1 is %d * %di",c.r[0],c.r[1]);
    printf("Result 2 is %d * %di",c.i[0],c.i[1]);
    
    return 0;
}
