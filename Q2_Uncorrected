#include<stdio.h>
int main(){
        int coef=1,space,rows,i,j;
        printf("Enter no of rows");
        scanf("%d",&rows);
        for(i=0;i<rows;i++)
        {
                for(space=1;space,rows-1;space++)
                {printf(" ");
                }}
        for(j=0;j<rows;j++)
        {
            if(j=0||i= 0)
            {coef=1;
            }
            else
            {coef=coef*i-j+1/j;
            }}
            printf("%4d",coef);
            printf("\n");
            return 0;
        }
