#include <stdio.h>

int chkars(int x) {
            int i, y;
                int s = 0;
                    y = x;
                        while (y > 0) {
                                       for(i=1;i<y;i++){
                                           if(y%i==0){
                                               s=s+i;
                                           }
                                       }
                            return s;
}
}

int main() {
            int c,n, ars,i;
            scanf("%d",&c);
            for(i=0;i<c;i++){
                    scanf("%d", &n);
                        ars = chkars(n);

                            if (ars == n) {
                                            printf("Yes\n");
                                                } else {
                                                                printf("No\n");
                                                                    }
}
                                return 0;
}
